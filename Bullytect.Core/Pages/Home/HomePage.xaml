<?xml version="1.0" encoding="UTF-8"?>
<d:MvxContentPage 
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="clr-namespace:MvvmCross.Forms.Core;assembly=MvvmCross.Forms"
    xmlns:local="clr-namespace:Bullytect.Core.Pages"
    x:Class="Bullytect.Core.Pages.Home.HomePage"
    xmlns:viewModels="clr-namespace:Bullytect.Core.ViewModels;assembly=Bullytect.Core"
    x:TypeArguments="viewModels:HomeViewModel"
    xmlns:i18n="clr-namespace:Bullytect.Core.I18N"
    xmlns:converters="clr-namespace:Bullytect.Core.Converters">
	<ContentPage.Content>
        <StackLayout Padding = "25,50">
            <Button x:Name="settingsButton" Text= "Settings" HorizontalOptions="Center" 
                    VerticalOptions="Center"/>
            <Button x:Name="logoutButton" Text="Logout" HorizontalOptions="Center"
                />
        </StackLayout>
    </ContentPage.Content>
</d:MvxContentPage>

